{
  "name": "dashboard-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/dashboard/api",
  "targets": {
    "build": {
      "command": "go build -o bin/dashboard-api ./cmd/server",
      "options": {
        "cwd": "apps/dashboard/api"
      }
    },
    "dev": {
      "command": "go run ./cmd/server",
      "options": {
        "cwd": "apps/dashboard/api"
      }
    },
    "test": {
      "command": "go test ./...",
      "options": {
        "cwd": "apps/dashboard/api"
      }
    }
  }
}
